	</rule>
	<rule>
		<from>^/([A-Za-z0-9_]+)/works/videos/class/([0-9]+)(/?)$</from>
		<to>/postShow|showPostListByClass.action?wKey=$1&amp;postType=1&amp;classID=$2</to>
	</rule>
	<rule>
		<from>^/u/([0-9]+)/works/videos/class/([0-9]+)(/?)$</from>
		<to>/postShow|showPostListByClass.action?userid=$1&amp;postType=1&amp;classID=$2</to>
	</rule>
	<outbound-rule>
		<from>^/postShow\|showPostListByClass.action\?postType=0&amp;classID=([0-9]+)$</from>
		<to>/works/images/class/$1/</to>
	</outbound-rule>
	<outbound-rule>
		<from>^/postShow\|showPostListByClass.action\?postType=1&amp;classID=([0-9]+)$</from>
		<to>/works/videos/class/$1/</to>
	</outbound-rule>
	<!-- 作品 结束 -->
	<!-- 私信 开始 -->
	<rule>
		<from>^/home/message(/?)$</from>
		<to>/messageUpdate|showMessageSummaryList.action</to>
	</rule>
	<outbound-rule>
		<from>^/messageUpdate\|showMessageSummaryList.action$</from>
		<to>/home/message/</to>
	</outbound-rule>
	<rule>
		<from>^/home/message/dialog/([0-9]+)(/?)$</from>
		<to>/messageUpdate|showMessageDialogueList.action?toUserId=$1</to>
	</rule>
	<outbound-rule>
		<from>^/messageUpdate\|showMessageDialogueList.action\?toUserId=([0-9]+)$</from>
