	</rule>
	<outbound-rule>
		<from>^/profileUpdate\|showXyProxy.action$</from>
		<to>/bizproxy/</to>
	</outbound-rule>
	<!-- 商业代理页 结束 -->
	<!-- 用户首页 开始-->
	<rule>
		<from>^/u/([1-9][0-9]{11})(/?)$</from>
		<to>/profileShow.action?userid=$1</to>
	</rule>
	<outbound-rule>
		<from>^/profileShow.action\?userid=([1-9][0-9]{11})$</from>
		<to>/u/$1/</to>
	</outbound-rule>
	<rule>
		<from>^/([A-Za-z0-9_]+)(/?)$</from>
		<to>/profileShow.action?wKey=$1</to>
	</rule>	
	<outbound-rule>
		<from>^/profileShow.action\?wKey=([A-Za-z0-9_]+)(/?)$</from>
		<to>/$1</to>
	</outbound-rule>
	<!-- 用户首页 结束-->
	<!-- 我的收藏 开始-->
	<rule>
		<from>^/home/favorites(/?)$</from>
		<to>/postUpdate|showCollectionPostList.action</to>
	</rule>	
	<outbound-rule>
		<from>^/postUpdate\|showCollectionPostList.action$</from>
		<to>/home/favorites/</to>
	</outbound-rule>
	<!-- 我的收藏 结束-->	
</urlrewrite>