		<from>^/xingyuShow\|showXingYuDetail.action\?xingyuID=([0-9]+)$</from>
		<to>/saying/details/$1.html</to>
	</outbound-rule>
	<!-- 星语 结束 -->
	<!-- 首页推荐位回顾 开始 -->
	<rule>
		<from>^/allbest(/?)$</from>
		<to>/index|showIndexAdReview.action</to>
	</rule>
	<outbound-rule>
		<from>^/index\|showIndexAdReview.action$</from>
		<to>/allbest/</to>
	</outbound-rule>
	<!-- 首页推荐位回顾 结束 -->
	<!-- 商业代理页 开始 -->
	<rule>
		<from>^/bizproxy(/?)$</from>
