<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"
        "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
<urlrewrite>
	<!--星云首页-->
	<rule>
		<from>^/$</from>
		<to>/index.action</to>
	</rule>
	<!-- 我的动态 开始 -->
	<rule>
		<from>^/home/activity(/?)$</from>
		<to>/myIndex.action</to>
	</rule>
	<outbound-rule>
		<from>^/myIndex.action$</from>
		<to>/home/activity/</to>
	</outbound-rule>
	<!-- 我的动态 结束 -->
	<!--新浪微连接回调 start-->
	<rule>
		<from>^/sina/callback</from>
		<to>/sinaOauth|callback.action</to>
	</rule>
	<!--新浪微连接回调 end-->
	<!-- 星作品 开始 -->
	<rule>
		<from>^/works.html$</from>
		<to>/index|showRecommendWorksList.action</to>
	</rule>
	<outbound-rule>
		<from>^/index\|showRecommendWorksList.action$</from>
		<to>/works.html</to>
	</outbound-rule>
	<rule>
		<from>^/works/([0-9]+).html$</from>
		<to>/index|showRecommendPindaoList.action?tradeId=$1</to>
	</rule>
	<outbound-rule>
		<from>^/index\|showRecommendPindaoList.action\?tradeId=([0-9]+)$</from>
		<to>/works/$1.html</to>
	</outbound-rule>
	<!-- 星作品 结束 -->
	<!-- 星云人才 开始 -->
	<rule>
		<from>^/professionals.html$</from>
		<to>/index|showRencaiList.action</to>
	</rule>
	<outbound-rule>
		<from>^/index\|showRencaiList.action$</from>
		<to>/professionals.html</to>
	</outbound-rule>
	<rule>
		<from>^/professionals_([0-9]+).html$</from>
		<to>/index|showRencaiList.action?tradeId=$1</to>
	</rule>
	<outbound-rule>
		<from>^/index\|showRencaiList.action\?tradeId=([0-9]+)$</from>
		<to>/professionals_$1.html</to>
	</outbound-rule>
	<outbound-rule>
		<from>^/index\|showRencaiListAjax.action\?tradeId=([0-9]+)$</from>
		<to>/professionals_$1.html</to>
	</outbound-rule>
	<!-- 星云人才 结束 -->
	<!-- 星感言 开始 -->
	<rule>
		<from>^/html/words/index.html$</from>
		<to>/html/words/index.jsp</to>
	</rule>
	<outbound-rule>
		<from>^/html/words/index.jsp$</from>
		<to>/html/words/index.html</to>
	</outbound-rule>
	<!-- 星感言 结束 -->
	<!-- 星云大侠 开始 -->
	<rule>
		<from>^/html/talent/index.html$</from>
		<to>/html/talent/index.jsp</to>
	</rule>
	<outbound-rule>
		<from>^/html/talent/index.jsp$</from>
		<to>/html/talent/index.html</to>
	</outbound-rule>
	<!-- 星云大侠 结束 -->
	<!-- 星云大片 开始 -->
	<rule>
		<from>^/html/image/index.html$</from>
		<to>/html/image/index.jsp</to>
	</rule>
	<outbound-rule>
		<from>^/html/image/index.jsp$</from>
		<to>/html/image/index.html</to>
	</outbound-rule>
	<!-- 星云大片 结束 -->
	<!-- 档案 开始 -->
	<rule>
		<from>^/([A-Za-z0-9_]+)/files(/?)$</from>
		<to>/profileShow|showProfileModule.action?wKey=$1</to>
	</rule>
	<rule>
		<from>^/u/([0-9]+)/files(/?)$</from>
		<to>/profileShow|showProfileModule.action?userid=$1</to>
	</rule>
	<outbound-rule>
		<from>^/profileShow\|showProfileModule.action$</from>
		<to>/files/</to>
	</outbound-rule>
	<rule>
		<from>^/([A-Za-z0-9_]+)/files/([0-9]+).html$</from>
		<to>/profileShow|showProfileModule.action?wKey=$1&amp;moduleID=$2</to>
	</rule>
	<rule>
		<from>^/u/([0-9]+)/files/([0-9]+).html$</from>
		<to>/profileShow|showProfileModule.action?userid=$1&amp;moduleID=$2</to>
	</rule>
	<outbound-rule>
		<from>^/profileShow\|showProfileModule.action\?moduleID=([0-9]+)$</from>
		<to>/files/$1.html</to>
	</outbound-rule>
	<rule>
		<from>^/([A-Za-z0-9_]+)/files/edit(/?)$</from>
		<to>/profileUpdate|editUserProfile.action?wKey=$1</to>
	</rule>
	<rule>
		<from>^/u/([0-9]+)/files/edit(/?)$</from>
		<to>/profileUpdate|editUserProfile.action?userid=$1</to>
	</rule>
	<outbound-rule>
		<from>^/profileUpdate\|editUserProfile.action$</from>
		<to>/files/edit/</to>
	</outbound-rule>
	<rule>
		<from>^/([A-Za-z0-9_]+)/files/edit/([0-9]+)(/?)$</from>
		<to>/profileUpdate|showProfileOtherByMoudle.action?wKey=$1&amp;moduleID=$2</to>
	</rule>
	<rule>
		<from>^/u/([0-9]+)/files/edit/([0-9]+)(/?)$</from>
		<to>/profileUpdate|showProfileOtherByMoudle.action?userid=$1&amp;moduleID=$2</to>
	</rule>
	<outbound-rule>
		<from>^/profileUpdate\|showProfileOtherByMoudle.action\?moduleID=([0-9]+)$</from>
		<to>/files/edit/$1/</to>
	</outbound-rule>
	<!-- 档案 结束 -->
	<!-- 作品 开始 -->
	<rule>
		<from>^/([A-Za-z0-9_]+)/works(/?)$</from>
		<to>/postShow|showPostList.action?wKey=$1&amp;postType=0</to>
	</rule>
	<rule>
		<from>^/u/([0-9]+)/works(/?)$</from>
